<<widget "clothingicon">><<nobr>>
	<<if $images is 1>>
		<<if $args[0].iconFile>>
			<<set $clothesiconFile = "img/misc/icon/clothes/" + $args[0].name_cap + ".png">>
			<<if $args[0].colour is 0>>
				<<print '<img id = "icon" alt= "" src ="' + $clothesiconFile + '">'>>
			<<else>>
				<<print '<img id = "icon" class = "icon-'+ $args[0].colour +'" alt= "" src ="' + $clothesiconFile + '">'>>
			<</if>>
		<</if>>
		<<if $args[0].accIcon isnot 0 and $args[0].accessory isnot 0>>
			<<set $clothesiconFile = "img/misc/icon/clothes/" + $args[0].name_cap + "_acc.png">>
			<span id = "spanIcon">
			<<if $args[0].accessory_colour is 0>>
				<<print '<img id = "accIcon" alt= "" src ="' + $clothesiconFile + '">'>>
			<<else>>
				<<print '<img id = "accIcon" class = "icon-'+ $args[0].accessory_colour +'" alt= "" src ="' + $clothesiconFile + '">'>>
				<</if>>
			</span>
		<</if>>

		
	<</if>>
<</nobr>><</widget>>