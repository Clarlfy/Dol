    <<print '<div class="tealbar" style="width:' + _percent + '%"></div>'>>
  <<else>>
    <<print '<div class="greenbar" style="width:' + _percent + '%"></div>'>>
  <</if>>
</div>
<</nobr>><<endwidget>>

