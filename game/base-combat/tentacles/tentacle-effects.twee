:: Widgets Tentacle Defense [widget]
<<widget "actionstentaclelefthand">><<nobr>>

<<if $leftarm is 0 and $tentacles[$tentacleindex].shaft isnot "finished">>
	<<if isSameTarget($leftactiondefault, { action: "lefthit", entity: "tentacle" + $tentacleindex })>>
| <label><span class="def">Strike the $tentacles[$tentacleindex].name tentacle</span> <<radiobutton "$leftaction" `{ action: "lefthit", entity: "tentacle" + $tentacleindex }` checked>></label>
	<<else>>
| <label><span class="def">Strike the $tentacles[$tentacleindex].name tentacle</span> <<radiobutton "$leftaction" `{ action: "lefthit", entity: "tentacle" + $tentacleindex }`>></label>
	<</if>>

	<<if isSameTarget($leftactiondefault, { action: "leftgrab", entity: "tentacle" + $tentacleindex })>>
| <label><span class="sub">Grab the $tentacles[$tentacleindex].name tentacle</span> <<radiobutton "$leftaction" `{ action: "leftgrab", entity: "tentacle" + $tentacleindex }` checked>></label>
	<<else>>
| <label><span class="sub">Grab the $tentacles[$tentacleindex].name tentacle</span> <<radiobutton "$leftaction" `{ action: "leftgrab", entity: "tentacle" + $tentacleindex }`>></label>
	<</if>>
<br>
<<elseif isSameTarget($leftarm, { entity: "tentacle" + $tentacleindex })>>You hold the $tentacles[$tentacleindex].name tentacle in your left hand.
<br>

	<<if isSameTarget($leftactiondefault, { action: "leftrub", entity: "tentacle" + $tentacleindex })>>
| <label><span class="sub">Milk it</span> <<radiobutton "$leftaction" `{ action: "leftrub", entity: "tentacle" + $tentacleindex }` checked>></label>
	<<else>>
| <label><span class="sub">Milk it</span> <<radiobutton "$leftaction" `{ action: "leftrub", entity: "tentacle" + $tentacleindex }`>></label>
