	<<if isSameTarget($leftactiondefault, { action: "leftstop", entity: "tentacle" + $tentacleindex })>>
| <label><span class="neutral">Let go</span> <<radiobutton "$leftaction" `{ action: "leftstop", entity: "tentacle" + $tentacleindex }` checked>></label>
	<<else>>
| <label><span class="neutral">Let go</span> <<radiobutton "$leftaction" `{ action: "leftstop", entity: "tentacle" + $tentacleindex }`>></label>
